{
    "name": "bhuvidya/laravel-parsedown-extra",
    "description": "Laravel Wrapper of Parsedown Extra.",
    "keywords": [
        "parsedown",
        "extra",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "http://parsedown.org/extra",
    "support": {
        "issues": "https://github.com/bhuvidya/laravel-parsedown-extra/issues",
        "source": "https://github.com/bhuvidya/laravel-parsedown-extra"
    },
    "authors": [
        {
            "name": "bhu Boue vidya",
            "email": "bhuvidya@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0",
        "php": ">=7.0",
        "erusev/parsedown-extra": "^0.7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4",
        "php": ">=7.1",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Bhuvidya\\ParsedownExtra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Bhuvidya\\ParsedownExtra\\Providers\\ParsedownExtraServiceProvider"
            ]
        }
    }
}
